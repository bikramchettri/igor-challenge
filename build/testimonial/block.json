{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "igor-blocks/igor-testimonial",
  "version": "0.1.0",
  "title": "Testimonial",
  "category": "igor-blocks",
  "icon": "smiley",
  "description": "Display testimonial from customers and clients on your website",
  "attributes": {
    "quote": {
      "type": "string",
      "selector": "p"
    },
    "name": {
      "type": "string",
      "selector": "h3"
    },
    "designation": {
      "type": "string",
      "selector": "p"
    }
  },
  "supports": {
    "html": false,
    "anchor": false,
    "customClassName": false
  },
  "textdomain": "igor-challenge",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}